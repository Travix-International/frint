{
  "name": "frint-example-counter",
  "version": "0.0.1",
  "description": "Counter example in frint",
  "main": "index.js",
  "scripts": {
    "bundle:vendors": "webpack --config ./vendors/webpack.config.js",
    "bundle:apps": "webpack",
    "bundle": "npm run bundle:vendors && npm run bundle:apps",
    "build": "npm run bundle",
    "start:server": "live-server ./build --port $(PORT)",
    "start": "npm run build && npm run start:server"
  },
  "author": "FrintJS Authors (https://github.com/frintjs/frint/graphs/contributors)",
  "license": "MIT",
  "dependencies": {
    "frint": "^3.3.1",
    "frint-react": "^3.3.1",
    "frint-store": "^3.3.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rxjs": "^5.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.9",
    "babel-preset-travix": "^1.1.0",
    "babel-register": "^6.9.0",
    "frint-config": "^3.3.1",
    "html-webpack-plugin": "^2.28.0",
    "live-server": "^1.2.0",
    "webpack": "^3.0.0"
  }
}
