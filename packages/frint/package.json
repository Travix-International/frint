{
  "name": "frint",
  "version": "2.6.0",
  "description": "Core plugin for Frint",
  "main": "lib/index.js",
  "homepage": "https://github.com/Travix-International/frint/tree/master/packages/frint-core",
  "scripts": {
    "lint": "eslint --color '{src,test}/**/*.js'",
    "transpile": "babel src --out-dir lib",
    "test": "mocha --colors --compilers js:babel-register --recursive ./src/**/*.spec.js",
    "cover:run": "nyc --reporter=json --require babel-register mocha --colors --compilers js:babel-register --recursive ./src/**/*.spec.js",
    "cover:report": "nyc report",
    "cover": "npm run cover:run && npm run cover:report",
    "dist:lib": "webpack --config ./webpack.config.js",
    "dist:min": "cross-env DIST_MIN=1 webpack --config ./webpack.config.js",
    "dist": "npm run dist:lib && npm run dist:min",
    "prepublish": "npm run transpile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Travix-International/frint.git"
  },
  "author": {
    "name": "Travix International B.V.",
    "url": "https://travix.com"
  },
  "keywords": [
    "frint"
  ],
  "dependencies": {
    "lodash": "^4.13.1",
    "rxjs": "^5.2.0",
    "travix-di": "^1.0.0"
  },
  "bugs": {
    "url": "https://github.com/Travix-International/frint/issues"
  },
  "license": "MIT",
  "types": "index.d.ts"
}
