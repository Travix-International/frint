{
  "extends": [
    "travix"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": 0,
    "no-restricted-imports": ["error", {
      "paths": [
        "lodash",
        "rxjs",
        "rxjs/add/*"
      ]
    }],
    "react/require-default-props": 0,
    "react/no-unused-prop-types": 0,
    "comma-dangle": ["error", "always-multiline"]
  }
}
